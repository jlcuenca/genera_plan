    "@angular/router": "^20.0.0",
    "bootstrap": "^5.3.7",
    "docx": "^9.5.1",
    "docxtemplater": "^3.65.3",
    "file-saver": "^2.0.5",
    "pizzip": "^3.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.3",
    "@angular/cli": "^20.0.3",
